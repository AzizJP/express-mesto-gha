{
  "extends": "airbnb-base",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "__filename",
          "__dirname",
          "__v"
        ]
      }
    ]
  }
}