{
  "extends": "airbnb-base",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "linebreak-style": ["off"],
    "implicit-arrow-linebreak": ["off"],
    "arrow-body-style": ["off"],
    "no-console":"off"
  }
}